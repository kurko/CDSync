target 'UDSync' do
  pod 'Alamofire', '~> 4'
  pod 'BrightFutures', '~> 5.1'
  pod 'SwiftyJSON', '~> 3.1'
end

target 'UDSyncTests' do
  pod 'Alamofire', '~> 4'
  pod 'BrightFutures', '~> 5.1'
  pod 'SwiftyJSON', '~> 3.1'
end
